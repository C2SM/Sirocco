&parallel_nml
 nproma             = 0
 nblocks_c          = 0     ! loop number of cell blocks
 nblocks_e          = 1     ! loop number of edge blocks
 nproma_sub         =     ! loop chunk length for radiation scheme
 p_test_run         = .false.
